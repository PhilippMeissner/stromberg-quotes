  "author": {
    "name": "Philipp Meissner",
    "url": "https://philippmeissner.dev"
  },
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "vite",
